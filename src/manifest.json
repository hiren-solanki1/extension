{
  "name": "Great Extension",
  "version": "1.0",
  "description": "Track your important time",
  "manifest_version": 3,
  "host_permissions": ["<all_urls>"],
  "permissions": [
    "activeTab",
    "webNavigation",
    "storage",
    "scripting",
    "cookies"
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "action": {
    "default_popup": "index.html"
  }
}
